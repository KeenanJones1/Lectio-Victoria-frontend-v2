ok\n\n\n const renderLists = () => {\n   return <select className=\"\" onChange={(event) => setSelectedList(event.target.value)} name=\"\" id=\"\" >\n     <option>Please select a list</option>\n     {props.readingLists.map(list => <option key={list.id} value={list.id}>{list.name}</option>)}\n   </select>\n}\n\nconsole.log(props)\n return (\n  <ModalContainer >\n   <div className=\"container\">\n    <div className=\"row\">\n     <div className=\"col-8 mx-auto col-md-6 col-lg-4 text-center text-captialize p-5\">\n       <div className=\"book-info\">\n        <h3>{title}</h3>\n        <h5>{authors.length ? authors.join(' & ') : authors}</h5>\n        <h6>Published {publishedDate} </h6>\n       </div>\n\n       <div className=\"book-img\">\n        {imageLinks ? <img src={imageLinks.thumbnail} className=\"book-img\"/> : <p>No image avaiable</p>}\n       </div>\n\n       <div className=\"row modal-btn py-3\">\n        <button className=\"col my-2\" onClick={() => makeBook()}>Add Book to List</button>\n        <button className=\"col my-2\" onClick={() => props.setOpen()}>Close</button>\n        {renderLists()}\n       </div>\n\n     </div>\n    </div>\n   </div>\n  </ModalContainer>\n )\n}\n\n\n\nexport default Modal\n","/Users/keenanjones/Desktop/LV-v2/frontend/src/components/readingLists.js",[],"/Users/keenanjones/Desktop/LV-v2/frontend/src/components/ReadingLists.js",[],"/Users/keenanjones/Desktop/LV-v2/frontend/src/components/ReadingList.js",[],"/Users/keenanjones/Desktop/LV-v2/frontend/src/pages/readingList.js",["107"],"import React, { Component } from 'react'\nimport Books from '../components/Books'\nimport ReadingBooks from '../components/readingBooks'\n\nclass readingList extends Component {\n constructor(){\n  super()\n  this.state={\n   readingBooks: [],\n   listInfo:{}\n  }\n }\n\n\n componentDidMount(){\n    const token = localStorage.getItem('token')\n    const reqObj = {\n        method: 'GET',\n        headers: {'Content-Type': 'application/json', 'Authorization': `Bearer ${token}`}}\n\n    fetch(`http://localhost:3000/reading_list/${this.props.routerProps.match.params.id}`, reqObj)\n    .then( resp => resp.json())\n    .then( data => this.setState({readingBooks: data.reading_list_books, listInfo: {name: data.name, id: data.id}}))\n }\n\n    renderBooks = () => {\n    return this.state.readingBooks.map( book => <ReadingBooks listID={this.state.listInfo.id} book={book.book} updateBooks={this.updateBooks}/>)\n    }\n\n    updateBooks = (booksArr) => {\n    this.setState({readingBooks: booksArr})\n    }\n\n\n render() {\n  return (\n   <div className=\"container\">\n    <h1>{this.state.listInfo.name ? this.state.listInfo.name : \"Please Wait\" }</h1>\n    <div className=\"row\">\n     {this.state.readingBooks.length ? this.renderBooks() :  null }\n    </div>\n   </div>\n  )\n }\n}\n\nexport default readingList\n\n","/Users/keenanjones/Desktop/LV-v2/frontend/src/components/readingBooks.js",[],"/Users/keenanjones/Desktop/LV-v2/frontend/src/components/Stats/Stats.js",["108"],"import React from 'react'\nimport Stat from './Stat'\n\nconst Stats = ({stats}) => {\n\n const renderStats = () => {\n  sortStats(stats);\n  return stats.map(stat => <Stat stat={stat}/> )\n }\n\n const sortStats = (stats) => {\n  let sortedStats = stats.sort((a,b) => b.level - a.level || b.value - a.value)\n }\n\n return (\n  <div className=\"\" id=\"stats-container\">\n   <h1>Stats</h1>\n   <div className=\"row\">\n   {renderStats()}\n   </div>\n  </div>\n )\n}\n\nexport default Stats\n","/Users/keenanjones/Desktop/LV-v2/frontend/src/components/Stats/Stat.js",[],"/Users/keenanjones/Desktop/LV-v2/frontend/src/components/Design.js",["109","110"],"import React from 'react'\nimport {useSpring, animated} from 'react-spring'\n\nconst Design = () => {\n\n const springProps = useSpring({ x: 100, from: { x: 0 } })\n\n\n return (\n  <div>\n\n  </div>\n )\n}\n\nexport default Design\n",{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","replacedBy":"114"},{"ru